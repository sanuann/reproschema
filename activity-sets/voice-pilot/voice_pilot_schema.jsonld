{
    "@context": [ "https://raw.githubusercontent.com/sanuann/schema-standardization/debug-schema-ui/contexts/generic.jsonld",
        "https://raw.githubusercontent.com/sanuann/schema-standardization/debug-schema-ui/activity-sets/voice-pilot/voice_pilot_context.jsonld"
    ],
    "@type": "https://raw.githubusercontent.com/sanuann/schema-standardization/debug-schema-ui/schemas/ActivitySet.jsonld",
    "@id": "voice_pilot_schema",
    "skos:prefLabel": "Voice Pilot Protocol Activity set",
    "skos:altLabel": "voice_pilot",
    "schema:description": "activity set for UW MIT Voice Pilot Protocol",
    "schema:schemaVersion": "0.0.1",
    "schema:version": "0.0.1",
    "schema:image": "https://raw.githubusercontent.com/sanuann/schema-standardization/debug-schema-ui/activity-sets/voice-pilot/mit_voice_pilot_applet_image.svg?sanitize=true",
    "variableMap": [
        {"variableName": "voice_screening_schema", "isAbout": "voice_screening_schema"},
        {"variableName": "demographics_schema", "isAbout": "demographics_schema"},
        {"variableName": "promis_global_schema", "isAbout": "promis_global_schema"},
        {"variableName": "gad7_schema", "isAbout": "gad7_schema"},
        {"variableName": "cage_aid_schema", "isAbout": "cage_aid_schema"},
        {"variableName": "bads_schema", "isAbout": "bads_schema"},
        {"variableName": "other_questions_schema", "isAbout": "other_questions_schema"},
        {"variableName": "voice_task_schema", "isAbout": "voice_task_schema"},
        {"variableName": "thankyou_schema", "isAbout": "thankyou_schema"}
    ],
    "ui": {
        "order": [
            "voice_screening_schema",
            "voice_task_schema",
            "demographics_schema",
            "promis_global_schema",
            "gad7_schema",
            "cage_aid_schema",
            "bads_schema",
            "other_questions_schema",
            "thankyou_schema"
        ],
        "shuffle": false,
        "activity_display_name": {
            "voice_screening_schema": "Screening",
            "demographics_schema": "Demographics",
            "promis_global_schema": "Health",
            "gad7_schema":  "Anxiety",
            "cage_aid_schema": "Substance use",
            "bads_schema": "Behavior",
            "other_questions_schema": "Other Questions",
            "voice_task_schema": "Voice Tasks",
            "thankyou_schema": "ThankYou"
        },
        "visibility":{
            "voice_screening_schema":true,
            "demographics_schema": true,
            "promis_global_schema":true,
            "gad7_schema":true,
            "cage_aid_schema":{
                "method":"POST",
                "url":"https://sig.mit.edu/vb/check",
                "payload":["voice_screening_schema"]
            },
            "bads_schema":{
                "method":"POST",
                "url":"https://sig.mit.edu/vb/check",
                "payload":["voice_screening_schema"]
            },
            "other_questions_schema":{
                "method":"POST",
                "url":"https://sig.mit.edu/vb/check",
                "payload":["voice_screening_schema"]
            },
            "voice_task_schema":{
                "method":"POST",
                "url":"https://sig.mit.edu/vb/check",
                "payload":["voice_screening_schema"]
            },
            "thankyou_schema": {
                "method":"POST",
                "url":"https://sig.mit.edu/vb/check",
                "payload":["voice_screening_schema"]
            }
        },
        "allow": ["autoAdvance", "disableBack"]
    }
}
